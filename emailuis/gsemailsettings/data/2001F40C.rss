#include <ecom/registryinfo.rh>
#include "../../../inc/emailversionnumbers.hrh" // KEmailEcomVersionNumber
#include "GSEmailSettingsPluginUids.hrh"

RESOURCE REGISTRY_INFO theInfo
	{
	dll_uid     = KFSEmailUiGsPluginDllUid;//0x2001F40C; // Plugin dll UID 
	interfaces  = 
		{
		INTERFACE_INFO 
			{
			interface_uid   = KGsPluginInterfaceUid;// UID for CGSPluginInterface - do not change. 0x10207236
			implementations = 
				{
				IMPLEMENTATION_INFO
					{
					implementation_uid  = KFSEmailUiGsPluginDllUid;//0x2001F40C; // Plugin UID 
					version_no          = KEmailEcomVersionNumber;
					display_name        = "EmailSettings"; // Plugin debug name
					default_data        = KGsParentPluginUid;//"0x10207237"; // Parent UID
					opaque_data         = "-1"; // Order number 
					}
				};
			}
		};
	}

