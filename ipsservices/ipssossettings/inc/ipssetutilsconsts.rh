    LLINK    itemmaxlength = KIpsSetUiMaxSettingsTextLength;
    LONG    itemflags = 0;
    LLINK   itemarray = 0;
    WORD    itemappereance = EIpsSetUiUndefined;
    }

STRUCT IPS_SET_ARRAY
    {
    STRUCT items[];
    }

