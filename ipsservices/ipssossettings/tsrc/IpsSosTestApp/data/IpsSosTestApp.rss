RESOURCE MENU_PANE r_testapp_menu
    {
    items=
        {		
		MENU_ITEM { command=ETestAppSubMenuId; txt="Run"; cascade=r_testapp_launch_sub_menu; },		
		MENU_ITEM { command=EAknCmdExit; txt="Exit"; }        			
        };
    }
    
    
