#include <ecom/registryinfo.rh>
#include "../../../inc/emailversionnumbers.hrh" // KEmailEcomVersionNumber

RESOURCE REGISTRY_INFO theInfo
	{
	// UID for the DLL
	dll_uid = 0x2001F462;
	
	// Declare array of interface info
	interfaces = 
		{
		INTERFACE_INFO
			{
			
			// UID of interface that is implemented
			interface_uid = 0x2000B5C0;
			
			implementations = 
				{
				
				// Info for all implementations					
				IMPLEMENTATION_INFO
					{
					implementation_uid = 0x2001F463;
					version_no         = KEmailEcomVersionNumber;
					display_name       = "PsMruAdapter"; // Identifies the MRU adapter
					default_data       = "";    // Data Store Id
					opaque_data        = ""; 
					}
					
				};
			}
		};
	}
	
