// r_mr_editor_attachment_menu
// Editor attachment field context menu pane
// ---------------------------------------------------------
//
RESOURCE MENU_PANE r_mr_editor_attachment_menu
    {
    items =
        {
        MENU_ITEM
            {
            command = EESMREditorAddAttachment;
            txt = qtn_meet_req_add_attachment;
            },
        MENU_ITEM
            {
            command = EESMREditorOpenAttachment;
            txt = qtn_meet_req_open_attachment;
            },
        MENU_ITEM
            {
            command = EESMREditorRemoveAttachment;
            txt = qtn_meet_req_remove_attachment;
            },
        MENU_ITEM
            {
            command = EESMREditorRemoveAllAttachments;
            txt = qtn_meet_req_remove_all_attachments;
            }
        };
    }

